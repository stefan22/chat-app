{
  "env": {
		"node": true,
    "commonjs": true,
    "es6": true
  },
  "plugins": ["react"],
  "extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-eval": "error",
    "no-undef": "error",
    "no-unreachable": "error",
    "no-unused-vars": "error"
   
  }
}